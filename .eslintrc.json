{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "parser": "@babel/eslint-parser",
  "rules": {
    "jsx-a11y/label-has-associated-control": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/no-autofocus": "off",
    "react/prop-types": "off",
    "no-unused-vars": "warn"
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  }
}